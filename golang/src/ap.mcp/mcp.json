{
    "brokerd": {
        "Name": "brokerd",
        "Binary": "ap.brokerd",
        "Privileged": false
    },
    "configd": {
        "Name": "configd",
        "Binary": "ap.configd",
        "DependsOn": "brokerd",
        "Privileged": true,
        "Options": "-propdir=$APROOT/etc"
    },
    "dhcp4d": {
        "Name": "dhcp4d",
        "Binary": "ap.dhcp4d",
        "DependsOn": "networkd",
        "Privileged": true
    },
    "dns4d": {
        "Name": "dns4d",
        "Binary": "ap.dns4d",
        "DependsOn": "configd",
        "Privileged": true
    },
    "filterd": {
        "Name": "filterd",
        "Binary": "ap.filterd",
        "DependsOn": "networkd",
        "Privileged": true,
	"Options": "-rdir=$APROOT/etc/filter.rules.d"
    },
    "networkd": {
        "Name": "networkd",
        "Binary": "ap.networkd",
        "DependsOn": "configd",
        "Privileged": true,
        "Options": "-template_dir=$APROOT/etc/templates/ap.networkd"
    },
    "httpd": {
        "Name": "httpd",
        "Binary": "ap.httpd",
        "DependsOn": "networkd",
        "Privileged": true,
        "Options": "-template_dir=$APROOT/etc/templates/ap.httpd -ssldir=$APROOT/etc/ssl"
    },
    "identifierd": {
        "Name": "identifierd",
        "Binary": "ap.identifierd",
        "DependsOn": "brokerd",
        "Privileged": true,
        "Options": "-datadir=$APROOT/etc -logdir $APROOT/var/spool/identifierd"
    },
    "logd": {
        "Name": "logd",
        "Binary": "ap.logd",
        "DependsOn": "brokerd",
        "Privileged": false
    },
    "sampled": {
        "Name": "sampled",
        "Binary": "ap.sampled",
        "DependsOn": "networkd",
        "Privileged": true
    },
    "scand": {
        "Name": "scand",
        "Binary": "ap.scand",
        "DependsOn": "networkd",
        "Privileged": true,
        "Options": "-scandir $APROOT/var/spool/scand"
    },
    "scand-ssdp": {
        "Name": "scand-ssdp",
        "Binary": "ap.scand-ssdp",
        "DependsOn": "configd",
        "Privileged": true
    },
    "prometheus": {
        "Name": "prometheus",
        "Binary": "prometheus",
        "Privileged": false,
        "ThirdParty": true,
	"Arch": "amd64",
	"Options": "-config.file=$APROOT/etc/prometheus.yml -storage.local.path=\"$APROOT/var/prometheus-data\""
    }
}
