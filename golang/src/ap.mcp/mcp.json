{
    "brokerd": {
        "Name": "brokerd",
        "Binary": "ap.brokerd",
        "Privileged": false
    },
    "configd": {
        "Name": "configd",
        "Binary": "ap.configd",
        "DependsOn": "brokerd",
        "Privileged": true,
        "Options": "-propdir=$APROOT/etc"
    },
    "dhcp4d": {
        "Name": "dhcp4d",
        "Binary": "ap.dhcp4d",
        "DependsOn": "networkd",
        "Privileged": true
    },
    "dns4d": {
        "Name": "dns4d",
        "Binary": "ap.dns4d",
        "DependsOn": "configd",
        "Privileged": true,
        "Options": "-root=$APROOT"
    },
    "dnsmasq": {
        "Name": "dnsmasq",
        "Binary": "/usr/sbin/dnsmasq",
        "DependsOn": "networkd",
        "Privileged": true,
        "ThirdParty": true,
        "Options": "-k -h -p 5300 -i lo"
    },
    "httpd": {
        "Name": "httpd",
        "Binary": "ap.httpd",
        "DependsOn": "networkd",
        "Privileged": true,
        "Options": "-root=$APROOT -template_dir=$APROOT/etc/templates/ap.httpd -client-web_dir=$APROOT/var/www/client-web"
    },
    "identifierd": {
        "Name": "identifierd",
        "Binary": "ap.identifierd",
        "DependsOn": "configd",
        "Privileged": true,
        "Options": "-datadir=$APROOT/etc -modeldir=$APROOT/etc/device_model -logdir $APROOT/var/spool/identifierd"
    },
    "logd": {
        "Name": "logd",
        "Binary": "ap.logd",
        "DependsOn": "brokerd",
        "Privileged": true,
        "Options": "-logdir=$APROOT/var/spool/logd"
    },
    "networkd": {
        "Name": "networkd",
        "Binary": "ap.networkd",
        "DependsOn": "configd",
        "Privileged": true,
        "Options": "-template_dir=$APROOT/etc/templates/ap.networkd -rdir=$APROOT/etc/filter.rules.d"
    },
    "prometheus": {
        "Name": "prometheus",
        "Binary": "/usr/local/bin/prometheus",
        "Privileged": true,
        "ThirdParty": true,
        "Options": "-config.file=$APROOT/etc/prometheus.yml -storage.local.path=\"$APROOT/var/prometheus-data\""
    },
    "relayd": {
        "Name": "relayd",
        "Binary": "ap.relayd",
        "DependsOn": "networkd",
        "Privileged": false
    },
    "watchd": {
        "Name": "watchd",
        "Binary": "ap.watchd",
        "DependsOn": "networkd",
        "Privileged": true,
        "Options": "-scandir $APROOT/var/spool/watchd"
    }
}
